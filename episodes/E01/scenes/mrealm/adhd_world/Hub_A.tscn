[sub_resource type="Shader" id=4]
code = "shader_type canvas_item;

void fragment() {
	vec4 back = texture(SCREEN_TEXTURE, SCREEN_UV);
	float intensity = max(0.4 - (UV.x * UV.y), 0.0);
	vec4 l = texture(TEXTURE, UV);
	vec3 a = back.rgb;
	vec3 b = l.rgb * l.a * intensity;
	vec3 fc = a + b;
	COLOR = vec4(fc, back.a);
}"

