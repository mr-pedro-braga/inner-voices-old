{

	"_supported_extensions": {
		"sc": "screenscript", "sson": "scenescript",
		"c": "c",
		"cpp": "c++",
		"ps": "proto-script-api",
		"js": "javascript",
		"json": "json",
		"jsav": "json",
		"gd": "gdscript",
		"html": "xml",
		"css": "css",
		"cmd": "batch",
		"md": "markdown"
	},

	"gdscript": {
		"primary": ["func", "const", "var", "enum", "class", "class_name", "tool",
		"extends", "yield", "await", "print", "match", "for", "in", "as", "while",
		"range", "as", "is", "and", "not", "or", "if", "else", "elif", "return",
		"void", "int", "float", "char", "bool", "export", "static", "pass",
		"true", "false", "self", "super", "await", "master", "puppet", "match",
		"mastersync", "puppetsync", "setget", "signal", "continue",],
		"secondary": ["Vector2", "Vector3"],
		"terciary": [],
	},
	"json": {"primary":["true", "false"], "secondary":[], "terciary":[]},
	"javascript": {"primary":["alert", "public", "private", "static", "function", "void", "int", "char", "boolean",
		"float", "document", "require", "import", "window", "var", "switch", "case",
		"in", "for", "while", "break", "continue", "if", "else", "return",
		"this", "try", "catch", "let", "true", "false"], "secondary":[], "terciary":[]
	},
	
	"scenescript": {
		"color_regions": [
			{"start": "#", "end": "", "color":"keyword"},
		],
		"primary": [
			"move", "choice", "mvto", "mvadd", "wait", "turn", "pose",
			"anim", "action", "face", "call", "switch", "var", "important",
			"continue", "back", "goto", "await",
