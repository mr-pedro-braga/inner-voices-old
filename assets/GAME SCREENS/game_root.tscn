[sub_resource type="GDScript" id=93]
script/source = "extends Node2D

func _process(_delta):
	update()

#
#	Draw some useful debugging information!
#

enum {
	FPS = 1,
	SWITCHES = 2,
}

export(int, FLAGS, \"FPS\", \"Switches\") var information
export var font: Font

func _draw():
	var line = 0
	if information & FPS:
		draw_string(font, position, \"FPS: \" + str(Utils.fps()).pad_decimals(1))
		line = line + 1
	if information & SWITCHES:
		for switch in Memory.switches.keys():
			var text = \"-- \\\"\" + switch + \"\\\" : \\t\" + str(Memory.check[switch])
			draw_string(font, position + Vector2(0, line * 10), text)
			line = line + 1
"

