[sub_resource type="Shader" id="9"]
code = "shader_type canvas_item;

uniform float chromatic_intensity = 10.0;
uniform float blur_intensity = 1.2f;

void fragment() {
	float d = (distance(SCREEN_UV, vec2(0.5, 0.5))/sqrt(2.0)*1.0);
	vec4 col = texture(SCREEN_TEXTURE, SCREEN_UV);
	col.rgb *= (1.0 - d*d*0.5);
	
	vec4 white = mix(col, vec4(1.0, 1.0, 1.0, 1.0), 0.0);
	
	if (abs(SCREEN_UV.x-1.0/3.0) < SCREEN_PIXEL_SIZE.x) col = white;
	if (abs(SCREEN_UV.x-2.0/3.0) < SCREEN_PIXEL_SIZE.x) col = white;
	if (abs(SCREEN_UV.y-1.0/3.0) < SCREEN_PIXEL_SIZE.y) col = white;
	if (abs(SCREEN_UV.y-2.0/3.0) < SCREEN_PIXEL_SIZE.y) col = white;
	
	COLOR = (col);
}"

